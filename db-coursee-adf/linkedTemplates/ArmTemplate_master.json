{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "db-coursee-adf"
		},
		"ls_covid_adfcourse_stge_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covid_adfcourse_stge_blob'"
		},
		"ls_covid_adfcourse_stge_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covid_adfcourse_stge_dl'"
		},
		"ls_f1_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_f1_storage'"
		},
		"ls_sql_covid_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_covid_db'"
		},
		"ls_covid_adfcourse_stge_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidadfcoursedl.dfs.core.windows.net/"
		},
		"ls_covid_http_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_dbcks_course_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1120-134324-23oo03m9"
		},
		"ls_f1_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula1dldbcoursee.dfs.core.windows.net/"
		},
		"tr_population_blob_to_dl_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/862554db-3cbc-4c84-9778-0c67a489d98e/resourceGroups/covid-adfcourse-rg/providers/Microsoft.Storage/storageAccounts/covidadfcoursesa"
		},
		"tr_process_f1_data_properties_pipeline_parameters_p_window_end_date": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "db-coursee-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_covid_adfcourse_stge_blob_connectionString": {
						"value": "[parameters('ls_covid_adfcourse_stge_blob_connectionString')]"
					},
					"ls_covid_adfcourse_stge_dl_accountKey": {
						"value": "[parameters('ls_covid_adfcourse_stge_dl_accountKey')]"
					},
					"ls_f1_storage_accountKey": {
						"value": "[parameters('ls_f1_storage_accountKey')]"
					},
					"ls_sql_covid_db_connectionString": {
						"value": "[parameters('ls_sql_covid_db_connectionString')]"
					},
					"ls_covid_adfcourse_stge_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_adfcourse_stge_dl_properties_typeProperties_url')]"
					},
					"ls_covid_http_github_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_http_github_properties_typeProperties_url')]"
					},
					"ls_dbcks_course_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_dbcks_course_properties_typeProperties_existingClusterId')]"
					},
					"ls_f1_storage_properties_typeProperties_url": {
						"value": "[parameters('ls_f1_storage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "db-coursee-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_blob_to_dl_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_blob_to_dl_properties_typeProperties_scope')]"
					},
					"tr_process_f1_data_properties_pipeline_parameters_p_window_end_date": {
						"value": "[parameters('tr_process_f1_data_properties_pipeline_parameters_p_window_end_date')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'db-coursee-adf_ArmTemplate_0')]"
			]
		}
	]
}